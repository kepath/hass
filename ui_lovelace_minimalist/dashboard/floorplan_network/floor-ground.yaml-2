      action: "none"
    hold_action:
      action: "none"
  # FRONT DOOR
  - type: "image"
    entity: "binary_sensor.lock_45f7_door"
    style:
      left: "50%"
      top: "50%"
      width: "100%"
    state_image:
      "on": "local/floorplan/ground/front_door.png"
